
;; Number operations

(number? 5)				; #t
(number? 'a)				; #f
(number? "abc")				; #f
(number? #t)				; #f
(number? '())				; #f
(number? (lambda (x) (* x x)))		; #f

(integer? 2)				; #t
(integer? #\a)				; #f

(=)					; #t
(= 1)					; #t
(= 1 1)					; #t
(= 1 2)					; #f
(= 1 1 1 1 1)				; #t
(= 1 1 1 1 2)				; #f

(<)					; #t
(< 1)					; #t
(< 1 2)					; #t
(< 1 1)					; #f
(< 1 -1)				; #f

(>)					; #t
(> 1)					; #t
(> 1 0 -1)				; #t
(> 2 1 2)				; #f

(<= 1 1 1 2)				; #t
(<=)					; #t
(<= 1 0 1 -10)				; #f
(<= -1000 -500 500)			; #t
